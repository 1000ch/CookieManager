{
	"name": "CookieManager",
	"version": "0.1.0",
	"manifest_version": 2,
	"description": "This extension provides an easy way to manage cookies.",
	"permissions": ["cookies", "*://*/*"],
	"browser_action": {
		"default_icon": "/src/icon/icon.png",
		"default_title": "CookieManager",
		"default_popup": "/src/html/popup.html"
	}
}