{
	"name": "CookieManager",
	"version": "0.1.2",
	"manifest_version": 2,
	"description": "An easy way to manage your browser cookies.",
	"permissions": ["cookies", "*://*/*"],
	"browser_action": {
		"default_icon": "/src/icon/icon.png",
		"default_title": "CookieManager",
		"default_popup": "/src/html/popup.html"
	}
}