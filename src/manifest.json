{
  "manifest_version": 2,
  "name": "CookieManager",
  "version": "0.2.1",
  "description": "An easy way to manage your browser cookies.",
  "permissions": [
    "cookies",
    "*://*/*"
  ],
  "browser_action": {
    "default_icon": "/icon/icon.png",
    "default_title": "CookieManager",
    "default_popup": "/html/popup.html"
  },
  "offline_enabled": true,
  "author": "1000ch"
}